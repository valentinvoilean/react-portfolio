{
  "presets": [
    [
      "airbnb",
      {
        "modules": false,
        "debug": false,
        "targets": {
          "browsers": [">1%", "last 4 versions", "Firefox ESR", "not ie < 10"]
        }
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        "transform-react-jsx-self",
        "transform-react-jsx-source"
      ]
    },
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
